{
  "name": "@eventscrape/api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "postbuild": "node ./scripts/flattenDist.mjs && node ./scripts/copyMigrations.mjs && node ./scripts/copyWorkerModules.mjs",
    "start": "node dist/server.js",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.3.1",
    "@fastify/rate-limit": "^9.1.0",
    "@google/generative-ai": "^0.21.0",
    "apify-client": "^2.9.3",
    "archiver": "^7.0.0",
    "axios": "^1.7.7",
    "better-sqlite3": "^11.0.0",
    "bullmq": "^4.15.4",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.29.1",
    "extract-zip": "^2.0.1",
    "fastify": "^4.24.3",
    "geolib": "^3.3.4",
    "ioredis": "^5.3.2",
    "jaro-winkler": "^0.2.8",
    "luxon": "^3.4.4",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "postgres": "^3.4.3",
    "string-similarity": "^4.0.4",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/better-sqlite3": "^7.6.11",
    "@types/luxon": "^3.3.7",
    "@types/string-similarity": "^4.0.2",
    "@types/uuid": "^9.0.7",
    "drizzle-kit": "^0.20.6",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.20.3",
    "typescript": "^5.3.0",
    "vitest": "^1.0.4"
  }
}
