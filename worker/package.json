{
  "name": "@eventscrape/worker",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/worker.ts",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && npm run copy-assets",
    "copy-assets": "cp src/modules/instagram/gemini-prompt.md dist/modules/instagram/",
    "start": "node dist/worker.js",
    "test": "vitest",
    "test:modules": "vitest modules",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "apify-client": "^2.9.3",
    "axios": "^1.7.7",
    "bullmq": "^4.15.4",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.29.1",
    "geolib": "^3.3.4",
    "instagram-private-api": "^1.46.1",
    "ioredis": "^5.3.2",
    "jaro-winkler": "^0.2.8",
    "luxon": "^3.4.4",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "playwright": "^1.40.1",
    "postgres": "^3.4.3",
    "string-similarity": "^4.0.4",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/luxon": "^3.3.7",
    "@types/string-similarity": "^4.0.2",
    "@types/uuid": "^9.0.7",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.6.2",
    "typescript": "^5.3.0",
    "vitest": "^1.0.4"
  }
}